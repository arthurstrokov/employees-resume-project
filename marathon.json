{
  "id": "employees-resume-project",
  "cmd": null,
  "cpus": 1.0,
  "mem": 512.0,
  "disk": 0,
  "instances": 1,
  "labels": {
    "HAPROXY_GROUP": "external"
  },
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "arthurstrokov/employees-resume-project:latest",
      "forcePullImage": true,
      "network": "BRIDGE"
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "hostPort": 8080,
        "containerPort": 8080,
        "servicePort": 0
      }
    ]
  },
  "upgradeStrategy": {
    "minimumHealthCapacity": 0,
    "maximumOverCapacity": 1
  },
  "healthChecks": [
    {
      "protocol": "COMMAND",
      "command": {
        "value": "pwd"
      },
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false
    },
    {
      "protocol": "TCP",
      "portNumber": 5432,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ]
}
